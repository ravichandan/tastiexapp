{
  "name": "foodiexapp",
  "version": "1.0.2",
  "main": "index.ts",
  "scripts": {
    "start": "expo start -c",
    "start-tunnel": "expo start -c --tunnel",
    "android": "env-cmd -f .env.android expo run:android --no-build-cache",
    "ios": "expo run:ios",
    "clean": "npm run deep-clean:android && npm run clean:ios && npm run prebuild",
    "clean:android": "rm -rf android .expo",
    "clean:ios": "rm -rf ios .expo",
    "deep-clean": "rm -rf node_modules && npm run deep-clean:android && npm run deep-clean:ios && npm install",
    "deep-clean:android": "rm -rf android .expo .gradle",
    "deep-clean:ios": "rm -rf ios Podfile.lock Pods",
    "prebuild": "npx expo prebuild --clean ",
    "web": "expo start --web",
    "format": "prettier --write .",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview --platform android",
    "build:configure": "eas build:configure",
    "build:ios": "eas build -p ios --profile production",
    "build:android": "eas build -p android --profile production",
    "submit:ios": "eas submit -p ios --profile production",
    "submit:android": "eas submit -p android --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "7.3.21",
    "@shopify/flash-list": "^2.0.2",
    "@tanstack/react-query": "5.81.5",
    "axios": "1.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "17.2.0",
    "expo": "~53.0.17",
    "expo-auth-session": "~6.2.1",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-location": "~18.1.6",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.525.0",
    "nativewind": "4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-pager-view": "^6.7.1",
    "react-native-paper": "^5.14.5",
    "react-native-paper-tabs": "0.11.4",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "10.2.0",
    "react-native-web": "0.20.0",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@svgr/cli": "^8.1.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "5.0.2",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "10.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.5.14",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
}
